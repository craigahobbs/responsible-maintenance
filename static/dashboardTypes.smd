# The Responsible Maintenance model
struct ResponsibleMaintenance

    # The package name
    string packageName

    # The language runtime
    string language

    # The package home URL
    string homeURL

    # The package links
    optional Link[len > 0] links

    # The package history
    ResponsibleMeasures[len > 0] history


# A link
struct Link
    string text
    string url


# A package's dependencies
struct ResponsibleMeasures

    # The date and time of the measurement
    datetime datetime

    # The package version
    string version

    # The total number of code lines
    int(> 0) lines

    # The number of code lines covered by unit tests
    int(> 0) linesCovered

    # The total number of code branches
    int(> 0) branches

    # The number of code branches covered by unit tests
    int(> 0) branchesCovered

    # The package's direct dependencies
    string[] dependencies

    # The package's total dependencies
    string[] dependenciesTotal
